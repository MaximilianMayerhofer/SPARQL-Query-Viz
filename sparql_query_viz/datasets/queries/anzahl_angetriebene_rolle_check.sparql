# Parameter f√ºr Anzahl angetriebener Rollen [MA4] entspricht nicht der Anzahl angetriebener Rollen aus [MA1] ROLDIA

SELECT DISTINCT ?anzahl (COUNT(DISTINCT ?r) AS ?rollcount)
WHERE {
    ?gz a :gieszanlage ;
        :has_param ?rollenanzahl ;
        :part/:part ?r .
    ?rollenanzahl a :anzahl_angetriebene_rollen ;
        :int_value ?anzahl.
    ?r a :angetriebene_rolle .
}
GROUP BY ?anzahl
HAVING ( ?anzahl != ?rollcount )
